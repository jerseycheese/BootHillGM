{
  "output": {
    "filePath": "repomix-output.txt",
    "style": "xml",
    "removeComments": false,
    "removeEmptyLines": false,
    "topFilesLength": 10,
    "showLineNumbers": false
  },
  "include": [],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "BootHillGMApp/package-lock.json",
      "Docs/ClosedBugs.md",
      "Docs/CompletedUserStories.md",
      "Docs/BootHillRules/*.md"
    ]
  }
}